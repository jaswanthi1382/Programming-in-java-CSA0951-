public class R192111382 {
    static int factorial(int n){
        if(n==0){
            return 1;
        }
        else{
            return n*factorial(n-1);
        }
    }
    public static void main(String[] args){
        int n=5;
        int fact=1,i;
        fact=factorial(n);
        System.out.println("factorial of "+n+"is:"+fact);
    }
}
