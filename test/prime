public class R1921191 {
    public static void main(String[] args){
        int n=50;
        System.out.println("prime numbers up to " +n+":");
        for(int i=2;i<=n;i++){
            if(isprime(i)){
                System.out.println(i);
            }
        }
    }
    private static boolean isprime(int number){
        for(int j=2;j<number;j++){
            if(number%j==0){
                return false;
            }
        }
        return true;
    }
}
