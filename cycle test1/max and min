import java.util.*;

public class maxmin {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int a[] = {12, 15, 87, 25, 89, 50};
        int max = a[0];
        int min = a[0];

        for (int i = 1; i < a.length; i++) {
            if (a[i] > max) {
                max = a[i];
            }
            if (a[i] < min) {
                min = a[i];
            }
        }

        System.out.println("Max: " + max);
        System.out.println("Min: " + min);
    }
}
